\define thisTable()
<table>
	<tr>
		<th>
			Selected
		</th>
		<th>
			Tiddler<<thisHelpButton tiddler_table_column_help>>
		</th>
		<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_tags' text='' tag=th>
			Tags<<thisHelpButton tags_table_column_help>>
		</$reveal>
		<$list filter='[[$:/temp/Generic Search and Replace/Search]show_fields[]]'>
			<th>
				Field Name<<thisHelpButton field_name_column_help>>
				<$reveal type='nomatch' state='$:/temp/Generic Search and Replace/Search!!show_field_contents' text=false>
					: Contents<<thisHelpButton field_contents_column_help>>
				</$reveal>
			</th>
		</$list>
	</tr>
	<$list filter='[[$:/temp/Generic Search and Replace/Search]has[search_tag]]'>
		<$list filter='$(IncludeSystem)$$(IncludeEmptyTiddlers)$$(IncludeSearchTiddler)$+[regexp[$(CaseSensitiveTitle)$$(TitleSearchString)$]regexp:tags[$(CaseSensitiveTags)$$(TagsSearchString)$]limit<TiddlerLimit>]+[sort[title]]' variable=MatchingTitle>
			<<thisTableBody>>
		</$list>
	</$list>
	<$list filter='[[$:/temp/Generic Search and Replace/Search]!has[search_tag]]'>
		<$list filter='$(IncludeSystem)$$(IncludeEmptyTiddlers)$$(IncludeSearchTiddler)$+[regexp[$(CaseSensitiveTitle)$$(TitleSearchString)$]limit<TiddlerLimit>]+[sort[title]]' variable=MatchingTitle>
			<<thisTableBody>>
		</$list>
	</$list>
</table>
\end

\define thisTableRowWithFields()
<td>
	<<thisSelectTiddlerButton>>
</td>
<td>
	<<thisRenameTiddlerButton>>
</td>
<$reveal type='nomatch' state='$:/temp/Generic Search and Replace/Search!!which_fields' text=all tag=tr>
	<<thisTableRowMatchingFields>>
</$reveal>
<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!which_fields' text=all tag=tr>
	<<thisTableRowAllFields>>
</$reveal>
\end

\define thisTableTiddlersWithNoFields()
<tr>
	<td>
		<<thisSelectTiddlerButton>>
	</td>
	<td>
		<<thisRenameTiddlerButton>>
	</td>
	<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_tags' text='' tag=td>
		<$set name=TagState value="""$:/temp/$(MatchingTitle)$/NewTagName""">
			<<thisEditTags>>
		</$set>
	</$reveal>
	<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_fields' text='' tag=td>
		<<thisAddFieldButton>>
	</$reveal>
</tr>
\end

\define thisTableRowMatchingFields()
<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_tags' text='' tag=td>
	<<thisEditTags>>
</$reveal>
<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_fields' text='' tag=td>
	<$list filter='[title<MatchingTitle>fields[]regexp[$(CaseSensitiveFields)$$(FieldSearchString)$]]$(IncludeText)$$(IncludeCreated)$$(IncludeModified)$$(IncludeType)$$(IncludeTags)$-[[title]]+[sort[title]]' variable=MatchingFields2>
		<$list filter='[<MatchingTitle>has<MatchingFields2>get<MatchingFields2>]+[sort[title]]' variable=MatchingFieldContents>
			''<<thisEditFieldTitleButton>>''<$reveal type='nomatch' state='$:/temp/Generic Search and Replace/Search!!show_field_contents' text=false>: <<thisEditFieldValueButton>></$reveal><br>
			<hr>
		</$list>
	</$list>
	<<thisAddFieldButton>>
</$reveal>
\end

\define thisTableRowAllFields()
<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_tags' text='' tag=td>
	<<thisEditTags>>
</$reveal>
<$reveal type='match' state='$:/temp/Generic Search and Replace/Search!!show_fields' text='' tag=td>
		<$list filter='[title<MatchingTitle>fields[]]$(IncludeText)$$(IncludeCreated)$$(IncludeModified)$$(IncludeType)$$(IncludeTags)$-[[title]]+[sort[title]]' variable=MatchingFields2>
			''<<thisEditFieldTitleButton>>''<$reveal type='nomatch' state='$:/temp/Generic Search and Replace/Search!!show_field_contents' text=false>: <<thisEditFieldValueButton>></$reveal><br>
			<hr>
		</$list>
	<<thisAddFieldButton>>
</$reveal>
\end

\define thisTableBody()
<$list filter='[[$:/temp/Generic Search and Replace/Search]has[field_name_search_text]]'>
	<$list filter='[[$:/temp/Generic Search and Replace/Search]has[search_text]]'>
		<$list filter='[title<MatchingTitle>fields[]regexp[$(CaseSensitiveFields)$$(FieldSearchString)$]]$(IncludeText)$$(IncludeCreated)$$(IncludeModified)$$(IncludeType)$$(IncludeTags)$-[[title]]+[sort[title]]+[limit[1]]' variable=MatchingFields>
			<$list filter='[<MatchingTitle>has<MatchingFields>get<MatchingFields>]+[sort[title]]' variable=MatchingFieldContents>					
				<<thisTableRowWithFields>>
			</$list>
		</$list>
	</$list>
	<$list filter='[[$:/temp/Generic Search and Replace/Search]!has[search_text]]'>
		<$list filter='[title<MatchingTitle>fields[]regexp[$(CaseSensitiveFields)$$(FieldSearchString)$]]$(IncludeText)$$(IncludeCreated)$$(IncludeModified)$$(IncludeType)$$(IncludeTags)$-[[title]]+[sort[title]]+[limit[1]]' variable=MatchingFields>
			<<thisTableRowWithFields>>
		</$list>
	</$list>
</$list>
<$list filter='[[$:/temp/Generic Search and Replace/Search]!has[field_name_search_text]]'>
	<$list filter='[[$:/temp/Generic Search and Replace/Search]has[search_text]]'>
		<$list filter='[title<MatchingTitle>fields[]]$(IncludeText)$$(IncludeCreated)$$(IncludeModified)$$(IncludeType)$$(IncludeTags)$-[[title]]+[sort[title]]+[limit[1]]' variable=MatchingFields>
			<$list filter='[<MatchingTitle>has<MatchingFields>get<MatchingFields>]+[sort[title]]' variable=MatchingFieldContents>					
				<<thisTableRowWithFields>>
			</$list>
		</$list>
	</$list>
	<$list filter='[[$:/temp/Generic Search and Replace/Search]!has[search_text]]'>
		<$list filter='[title<MatchingTitle>fields[]]$(IncludeText)$$(IncludeCreated)$$(IncludeModified)$$(IncludeType)$$(IncludeTags)$-[[title]]+[sort[title]]+[limit[1]]' variable=MatchingFields emptyMessage=<<thisTableTiddlersWithNoFields>>>
			<tr>
				<td>
					<<thisSelectTiddlerButton>>
				</td>
				<td>
					<<thisRenameTiddlerButton>>
				</td>
				<<thisTableRowAllFields>>
			</tr>
		</$list>
	</$list>
</$list>
\end

<$vars SearchTiddler='$:/plugins/inmysocks/TiddlerTool/TiddlerTool' IncludeSearchTiddler={{$:/temp/Generic Search and Replace/Search!!include_this_tiddler}} ShowFields={{$:/temp/Generic Search and Replace/Search!!show_fields}} IncludeSystem={{$:/temp/Generic Search and Replace/Search!!include_system_tiddlers}} IncludeTags={{$:/temp/Generic Search and Replace/Search!!include_tags}} IncludeText="-[[text]]" IncludeCreated={{$:/temp/Generic Search and Replace/Search!!include_created}} IncludeModified={{$:/temp/Generic Search and Replace/Search!!include_modified}} IncludeType={{$:/temp/Generic Search and Replace/Search!!include_type}} CaseSensitiveTitle={{$:/temp/Generic Search and Replace/Search!!case_sensitive_title}} CaseSensitiveTags={{$:/temp/Generic Search and Replace/Search!!case_sensitive_tags}} CaseSensitiveFields={{$:/temp/Generic Search and Replace/Search!!case_sensitive_fields}} CaseSensitiveContents={{$:/temp/Generic Search and Replace/Search!!case_sensitive_contents}} TitleSearchString={{$:/temp/Generic Search and Replace/Search!!tiddler_search_text}} TagsSearchString={{$:/temp/Generic Search and Replace/Search!!search_tag}} FieldSearchString={{$:/temp/Generic Search and Replace/Search!!field_name_search_text}} ContentsSearchString={{$:/temp/Generic Search and Replace/Search!!search_text}} TiddlerLimit=10000>
	<<thisTable>>
</$vars>