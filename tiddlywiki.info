{
    "description": "Server configuration of the Maskott TW5 edition",
    "plugins": [
        "tiddlywiki/filesystem",
        "tiddlywiki/tiddlyweb",
        "tiddlywiki/internals",
        "tiddlywiki/highlight",
        "tiddlywiki/codemirror",
        "tiddlywiki/codemirror-closetag",
        "tiddlywiki/codemirror-autocomplete",
        "tiddlywiki/codemirror-closebrackets",
        "tiddlywiki/codemirror-fullscreen-editing",
        "tiddlywiki/codemirror-search-replace",
        "tiddlywiki/codemirror-mode-javascript",
        "tiddlywiki/codemirror-mode-markdown",
        "tiddlywiki/codemirror-mode-xml",
        "tiddlywiki/github-fork-ribbon",
        "tiddlywiki/katex",
        "tiddlywiki/comments"
    ],  
	"languages": [
		"fr-FR"
	],    
    "themes": [
        "tiddlywiki/vanilla",
        "tiddlywiki/snowwhite"
    ],
	"build": {
		"index": [
			"--rendertiddler","$:/plugins/tiddlywiki/tiddlyweb/save/offline","index.html","text/plain"],
		"favicon": [
			"--savetiddler","$:/favicon.ico","favicon.ico"],
		"readme": [
			"--rendertiddler","ReadMe","readme.md","text/html",
			"--rendertiddler","License","license.md","text/html"]
	}       
}