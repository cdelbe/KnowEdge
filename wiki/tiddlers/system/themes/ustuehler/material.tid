        "$:/themes/ustuehler/material/ui/PageTemplate/toolbar": {
            "title": "$:/themes/ustuehler/material/ui/PageTemplate/toolbar",
            "created": "20171102053758393",
            "creator": "ustuehler",
            "modified": "20171120034225135",
            "modifier": "ustuehler",
            "span": "12",
            "type": "text/vnd.tiddlywiki",
            "text": "<$set name=\"siteTitle\" value={{$:/SiteTitle}}>\n\n<header class=\"mdc-toolbar mdc-toolbar--fixed\">\n\n<div class=\"mdc-toolbar__row\">\n\n<section class=\"mdc-toolbar__section mdc-toolbar__section--align-start\">\n\n<span class=\"catalog-back\">\n\n<$transclude tiddler=\"$:/themes/ustuehler/material/ui/PageTemplate/toolbar-icon\" mode=\"inline\"/>\n\n</span>\n\n<$transclude tiddler=\"$:/themes/ustuehler/material/ui/PageTemplate/toolbar-title\" mode=\"inline\"/>\n\n</section>\n\n<!-- TODO: add a search field. editing the search field should load the matching tiddlers after a delay, if the list is short enough.\n\n<div class=\"mdc-textfield\">\n  <input type=\"text\" id=\"textfield-no-js\" class=\"mdc-textfield__input\" placeholder=\"Search by Title or Text\">\n  <div class=\"mdc-textfield__bottom-line\"></div>\n</div>\n\n-->\n\n<section class=\"mdc-toolbar__section mdc-toolbar__section--align-end mdc-toolbar__section--shrink-to-fit\">\n\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/PageTemplate/ToolbarRight]!has[draft.of]]\">\n\n<$transclude mode=\"inline\"/>\n\n</$list>\n\n</section>\n\n</div>\n\n</header>\n\n</$set>"
        },
        "$:/themes/ustuehler/material/ui/PageTemplate": {
            "title": "$:/themes/ustuehler/material/ui/PageTemplate",
            "text": "\\define containerClasses()\ntc-page-container tc-page-view-$(themeTitle)$ tc-language-$(languageTitle)$ mdc-typography\n\\end\n\n<$importvariables filter=\"[[$:/core/ui/PageMacros]] [all[shadows+tiddlers]tag[$:/tags/Macro]!has[draft.of]]\">\n\n<$set name=\"tv-config-toolbar-icons\" value={{$:/config/Toolbar/Icons}}>\n\n<$set name=\"tv-config-toolbar-text\" value={{$:/config/Toolbar/Text}}>\n\n<$set name=\"tv-config-toolbar-class\" value={{$:/config/Toolbar/ButtonClass}}>\n\n<$set name=\"themeTitle\" value={{$:/view}}>\n\n<$set name=\"currentTiddler\" value={{$:/language}}>\n\n<$set name=\"languageTitle\" value={{!!name}}>\n\n<$set name=\"currentTiddler\" value=\"\">\n\n<div class=<<containerClasses>>>\n\n<$navigator story=\"$:/StoryList\" history=\"$:/HistoryList\" openLinkFromInsideRiver={{$:/config/Navigation/openLinkFromInsideRiver}} openLinkFromOutsideRiver={{$:/config/Navigation/openLinkFromOutsideRiver}} relinkOnRename={{$:/config/RelinkOnRename}}>\n\n<$dropzone>\n\n<$transclude tiddler=\"$:/themes/ustuehler/material/ui/PageTemplate/toolbar\" mode=\"inline\"/>\n\n<$transclude tiddler=\"$:/themes/ustuehler/material/ui/PageTemplate/drawer\" mode=\"inline\"/>\n\n<main class=\"mdc-toolbar-fixed-adjust\" style=\"width:100%;\">\n\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/PageTemplate]!has[draft.of]]\" variable=\"listItem\">\n\n<$transclude tiddler=<<listItem>>/>\n\n</$list>\n\n</main>\n\n</$dropzone>\n\n<$snackbar/>\n\n</$navigator>\n\n</div>\n\n</$set>\n\n</$set>\n\n</$set>\n\n</$set>\n\n</$set>\n\n</$set>\n\n</$set>\n\n</$importvariables>"
        },
        "$:/themes/ustuehler/material/ui/ViewTemplate/title": {
            "title": "$:/themes/ustuehler/material/ui/ViewTemplate/title",
            "created": "20171027022935772",
            "modified": "20171027023546869",
            "type": "text/vnd.tiddlywiki",
            "text": "\\define title-styles()\nfill:$(foregroundColor)$;\n\\end\n\\define config-title()\n$:/config/ViewToolbarButtons/Visibility/$(listItem)$\n\\end\n<div class=\"tc-tiddler-title\">\n<div class=\"tc-titlebar\">\n<span class=\"tc-tiddler-controls\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/ViewToolbar/Title]!has[draft.of]]\" variable=\"listItem\"><$reveal type=\"nomatch\" state=<<config-title>> text=\"hide\"><$set name=\"tv-config-toolbar-class\" filter=\"[<tv-config-toolbar-class>] [<listItem>encodeuricomponent[]addprefix[tc-btn-]]\"><$transclude tiddler=<<listItem>>/></$set></$reveal></$list>\n</span>\n<$set name=\"tv-wikilinks\" value={{$:/config/Tiddlers/TitleLinks}}>\n<$link>\n<$set name=\"foregroundColor\" value={{!!color}}>\n<span class=\"tc-tiddler-title-icon\" style=<<title-styles>>>\n<$transclude tiddler={{!!icon}}/>\n</span>\n</$set>\n<$list filter=\"[all[current]removeprefix[$:/]]\">\n<h2 class=\"tc-title\" title={{$:/language/SystemTiddler/Tooltip}}>\n<span class=\"tc-system-title-prefix\">$:/</span><$text text=<<currentTiddler>>/>\n</h2>\n</$list>\n<$list filter=\"[all[current]!prefix[$:/]]\">\n<h2 class=\"tc-title\">\n<$reveal state=\"$:/themes/ustuehler/material/options/captiontitles\" type=\"match\" text=\"yes\">\n<$view field=\"caption\">\n<$view field=\"title\"/>\n</$view>\n</$reveal>\n<$reveal state=\"$:/themes/ustuehler/material/options/captiontitles\" type=\"nomatch\" text=\"yes\">\n<$view field=\"title\"/>\n</$reveal>\n</h2>\n</$list>\n</$link>\n</$set>\n</div>\n\n<$reveal type=\"nomatch\" text=\"\" default=\"\" state=<<tiddlerInfoState>> class=\"tc-tiddler-info tc-popup-handle\" animate=\"yes\" retain=\"yes\">\n\n<$transclude tiddler=\"$:/core/ui/TiddlerInfo\"/>\n\n</$reveal>\n</div>"
        },
        "$:/themes/ustuehler/material/ui/ViewTemplate": {
            "title": "$:/themes/ustuehler/material/ui/ViewTemplate",
            "created": "20171101004327074",
            "modified": "20171101004517483",
            "type": "text/vnd.tiddlywiki",
            "view-style": "color: red",
            "text": "\\define frame-classes()\ntc-tiddler-frame tc-tiddler-view-frame mdc-elevation--z1 $(grid-item-class)$ $(missingTiddlerClass)$ $(shadowTiddlerClass)$ $(systemTiddlerClass)$ $(tiddlerTagClasses)$\n\\end\n\\define frame-style()\n$(tiddlerStyle)$\n\\end\n\\define folded-state()\n$:/state/folded/$(currentTiddler)$\n\\end\n<$set name=\"tiddlerStyle\" value={{!!view-style}}><$set name=\"storyTiddler\" value=<<currentTiddler>>><$set name=\"tiddlerInfoState\" value=<<qualify \"$:/state/popup/tiddler-info\">>><$tiddler tiddler=<<currentTiddler>>><div class=<<grid-item-class \"grid-item\">>><div class=<<frame-classes>> style=<<frame-style>>><$list filter=\"[all[shadows+tiddlers]tag[$:/tags/ViewTemplate]!has[draft.of]]\" variable=\"listItem\"><$transclude tiddler=<<listItem>>/></$list>\n</div></div>\n</$tiddler></$set></$set></$set>"
        },
        "$:/themes/ustuehler/material/ui/ViewToolbar": {
            "title": "$:/themes/ustuehler/material/ui/ViewToolbar",
            "created": "20171031193700249",
            "modified": "20171101030445553",
            "tags": "$:/tags/ViewTemplate",
            "type": "text/vnd.tiddlywiki",
            "text": "<div class=\"tc-tiddler-controls-bar\">\n<span class=\"tc-view-toolbar--bottom-left\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/ViewToolbar/BottomLeft]!has[draft.of]]\" variable=\"listItem\"><$reveal type=\"nomatch\" state=<<config-title>> text=\"hide\"><$set name=\"tv-config-toolbar-class\" filter=\"[<tv-config-toolbar-class>] [<listItem>encodeuricomponent[]addprefix[tc-btn-]]\"><$transclude tiddler=<<listItem>>/></$set></$reveal></$list>\n</span>\n<span class=\"tc-view-toolbar--bottom-right\" style=\"float: right;\">\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/ViewToolbar/BottomRight]!has[draft.of]]\" variable=\"listItem\"><$reveal type=\"nomatch\" state=<<config-title>> text=\"hide\"><$set name=\"tv-config-toolbar-class\" filter=\"[<tv-config-toolbar-class>] [<listItem>encodeuricomponent[]addprefix[tc-btn-]]\"><$transclude tiddler=<<listItem>>/></$set></$reveal></$list>\n</span>\n</div>"
        }
    }
