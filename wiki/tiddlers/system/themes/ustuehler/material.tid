dicator>>;\n}\n\n/*\n** File inputs\n*/\n\n.tc-file-input-wrapper {\n\tposition: relative;\n\toverflow: hidden;\n\tdisplay: inline-block;\n\tvertical-align: middle;\n}\n\n.tc-file-input-wrapper input[type=file] {\n\tposition: absolute;\n\ttop: 0;\n\tleft: 0;\n\tright: 0;\n\tbottom: 0;\n\tfont-size: 999px;\n\tmax-width: 100%;\n\tmax-height: 100%;\n\tfilter: alpha(opacity=0);\n\topacity: 0;\n\toutline: none;\n\tbackground: white;\n\tcursor: pointer;\n\tdisplay: inline-block;\n}\n\n/*\n** Thumbnail macros\n*/\n\n.tc-thumbnail-wrapper {\n\tposition: relative;\n\tdisplay: inline-block;\n\tmargin: 6px;\n\tvertical-align: top;\n}\n\n.tc-thumbnail-right-wrapper {\n\tfloat:right;\n\tmargin: 0.5em 0 0.5em 0.5em;\n}\n\n.tc-thumbnail-image {\n\ttext-align: center;\n\toverflow: hidden;\n\tborder-radius: 3px;\n}\n\n.tc-thumbnail-image svg,\n.tc-thumbnail-image img {\n\tfilter: alpha(opacity=1);\n\topacity: 1;\n\tmin-width: 100%;\n\tmin-height: 100%;\n\tmax-width: 100%;\n}\n\n.tc-thumbnail-wrapper:hover .tc-thumbnail-image svg,\n.tc-thumbnail-wrapper:hover .tc-thumbnail-image img {\n\tfilter: alpha(opacity=0.8);\n\topacity: 0.8;\n}\n\n.tc-thumbnail-background {\n\tposition: absolute;\n\tborder-radius: 3px;\n}\n\n.tc-thumbnail-icon svg,\n.tc-thumbnail-icon img {\n\twidth: 3em;\n\theight: 3em;\n\t<<filter \"drop-shadow(2px 2px 4px rgba(0,0,0,0.3))\">>\n}\n\n.tc-thumbnail-wrapper:hover .tc-thumbnail-icon svg,\n.tc-thumbnail-wrapper:hover .tc-thumbnail-icon img {\n\tfill: #fff;\n\t<<filter \"drop-shadow(3px 3px 4px rgba(0,0,0,0.6))\">>\n}\n\n.tc-thumbnail-icon {\n\tposition: absolute;\n\ttop: 0;\n\tleft: 0;\n\tright: 0;\n\tbottom: 0;\n\tdisplay: -webkit-flex;\n\t-webkit-align-items: center;\n\t-webkit-justify-content: center;\n\tdisplay: flex;\n\talign-items: center;\n\tjustify-content: center;\n}\n\n.tc-thumbnail-caption {\n\tposition: absolute;\n\tbackground-color: #777;\n\tcolor: #fff;\n\ttext-align: center;\n\tbottom: 0;\n\twidth: 100%;\n\tfilter: alpha(opacity=0.9);\n\topacity: 0.9;\n\tline-height: 1.4;\n\tborder-bottom-left-radius: 3px;\n\tborder-bottom-right-radius: 3px;\n}\n\n.tc-thumbnail-wrapper:hover .tc-thumbnail-caption {\n\tfilter: alpha(opacity=1);\n\topacity: 1;\n}\n\n/*\n** Errors\n*/\n\n.tc-error {\n\tbackground: #f00;\n\tcolor: #fff;\n}\n\n/*\n** Tree macro\n*/\n\n.tc-tree div {\n    \tpadding-left: 14px;\n}\n\n.tc-tree ol {\n    \tlist-style-type: none;\n    \tpadding-left: 0;\n    \tmargin-top: 0;\n}\n\n.tc-tree ol ol {\n    \tpadding-left: 1em;    \n}\n\n.tc-tree button { \n    \tcolor: #acacac;\n}\n\n.tc-tree svg {\n     \tfill: #acacac;\n}\n\n.tc-tree span svg {\n    \twidth: 1em;\n    \theight: 1em;\n    \tvertical-align: baseline;\n}\n\n.tc-tree li span {\n    \tcolor: lightgray;\n}\n\n/*\n * Make the sidebar span the entire page height\n */\n\n.tc-sidebar-scrollable {\n  display: grid;\n  position: relative;\n  margin: 0;\n  padding: 0;\n}\n\n/*\n.mdc-permanent-drawer.tc-sidebar-scrollable > div {\n  position: fixed;\n}\n*/\n\n.tc-dropzone {\n  display: flex;\n}\n\n.sidebar-wrapper {\n  position: fixed;\n  display: flex;\n  flex-flow: column;\n  height: 100%;\n  width: 100%;\n}\n\n.sidebar-content {\n  flex: 0 1 auto;\n  width: 100%;\n}\n\n.sidebar-footer {\n  flex: 1 1 auto;\n}\n\n/*\n * Show tiddler controls only when hovering over the tiddler, except for the\n * \"star\" button, which is always shown when the current tiddler is starred\n */\n\n.tc-tiddler-frame .tc-titlebar .tc-tiddler-controls button {\n  opacity: 0;\n  transition: opacity 0.5s;\n}\n.tc-tiddler-frame:hover .tc-titlebar .tc-tiddler-controls button {\n  opacity: 1;\n  transition: opacity 0.5s;\n}\n/* Always show the star button when the current tiddler is starred */\n.tc-tiddler-frame .tc-titlebar .tc-tiddler-controls .starred button {\n  opacity: 1;\n}\n\n/*\n * Show the \"star\" button's icon in a special colour when the current tiddler\n * is starred\n */\n\n.tc-tiddler-frame .tc-titlebar .tc-tiddler-controls .starred button i {\n  color: blue;\n}\n\n/*\n * Make link text paragraph in FAB fill the entire height.\n */\n\na.mdc-fab__icon {\n  height: 100%;\n}\n\n/*\n * This will position the FAB in the bottom-right corner.\n * Modify to fit your design's requirements.\n *\n * ref: https://github.com/material-components/material-components-web/tree/master/packages/mdc-fab#absolutely-positioned\n */\n\n.tc-fab--absolute {\n  position: fixed !important;\n  bottom: 1rem;\n  right: 1rem;\n}\n\n@media(min-width: 1024px) {\n  .tc-fab--absolute {\n    bottom: 1.5rem;\n    right: 1.5rem;\n  }\n}\n\n/*\n * Hide the snackbar completely when it is not active\n */\n\ndiv[aria-hidden=\"true\"].mdc-snackbar {\n  opacity: 0;\n}\n\n/* Display the snackbar at notification level */\ndiv.mdc-snackbar {\n\tz-index: 10000;\n}\n\n/*\n * Hide the outline that may be shown briefly around buttons when they're clicked\n */\n\nbutton {\n  outline: 0;\n}\n\n/*\n * Elevate the tiddler being hovered over\n */\n\n.tc-tiddler-frame:not(.hover) {\n  transition: all 0.5s linear;\n}\n\n.tc-tiddler-frame:hover {\n  /* XXX: copied from mdc-elevation--z10 */\n  -webkit-box-shadow: 0 6px 6px -3px rgba(0,0,0,.2), 0 10px 14px 1px rgba(0,0,0,.14), 0 4px 18px 3px rgba(0,0,0,.12);\n  box-shadow: 0 6px 6px -3px rgba(0,0,0,.2), 0 10px 14px 1px rgba(0,0,0,.14), 0 4px 18px 3px rgba(0,0,0,.12);\n  transition: all 0.5s linear;\n}\n\n/*\n** Tiddler Controls Bar below the main tiddler view template\n*/\n\n.tc-tiddler-frame .tc-tiddler-controls-bar {\n  opacity: 0;\n  color: rgba(0,0,0, 0.5);\n  transition: all 0.5s linear;\n}\n.tc-tiddler-frame:hover .tc-tiddler-controls-bar {\n  opacity: 1;\n  transition: all 0.5s linear;\n}\n\n.tc-tiddler-frame .tc-view-toolbar--bottom-left button {\n  margin-right: 8px;\n}\n\n.tc-tiddler-frame .tc-view-toolbar--bottom-right button {\n  margin-left: 8px;\n}\n\n/*\n** Muuri grid styling\n**\n** ref: https://github.com/haltu/muuri#table-of-contents\n*/\n\n.grid {\n  position: relative;\n}\n.grid-item {\n  display: block;\n  position: absolute;\n  width: 100%;\n  margin: 8px;\n  z-index: 1;\n}\n.grid-item.muuri-dragging {\n  z-index: 3;\n}\n.grid-item.muuri-releasing {\n  z-index: 2;\n}\n.grid-item.muuri-hidden {\n  z-index: 0;\n}\n.grid-item-content {\n  position: relative;\n  width: 100%;\n  height: 100%;\n}\n\n/*\n** Tiddler view styles\n*/\n\n.grid-item.span-12 { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-11 { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-10 { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-9  { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-8  { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-7  { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-6  { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item.span-5  { width: calc(100% /  1 - 16px); min-width: calc(72px *  5); }\n.grid-item,\n.grid-item.span-4  { width: calc(100% /  1 - 16px); min-width: calc(72px *  4); }\n.grid-item.span-3  { width: calc(100% /  1 - 16px); min-width: calc(72px *  3); }\n.grid-item.span-2  { width: calc(100% /  1 - 16px); min-width: calc(72px *  2); }\n.grid-item.span-1  { width: calc(100% /  1 - 16px); min-width: calc(72px *  1); }\n\n/* Screens wide enough to accomodate two rows */\n@media (min-width: {{$:/themes/ustuehler/material/metrics/sidebarbreakpoint}}) {\n  .grid-item.span-12 { width: calc(100% /  1 - 16px); min-width: calc(72px * 12); }\n  .grid-item.span-11 { width: calc(100% /  1 - 16px); min-width: calc(72px * 11); }\n  .grid-item.span-10 { width: calc(100% /  1 - 16px); min-width: calc(72px * 10); }\n  .grid-item.span-9  { width: calc(100% /  1 - 16px); min-width: calc(72px *  9); }\n  .grid-item.span-8  { width: calc(100% /  1 - 16px); min-width: calc(72px *  8); }\n  .grid-item.span-7  { width: calc(100% /  1 - 16px); min-width: calc(72px *  7); }\n  .grid-item.span-6  { width: calc(100% /  2 - 16px); min-width: calc(72px *  6); }\n  .grid-item.span-5  { width: calc(100% /  2 - 16px); min-width: calc(72px *  5); }\n  .grid-item,\n  .grid-item.span-4  { width: calc(100% /  2 - 16px); min-width: calc(72px *  4); }\n  .grid-item.span-3  { width: calc(100% /  2 - 16px); min-width: calc(72px *  3); }\n  .grid-item.span-2  { width: calc(100% /  2 - 16px); min-width: calc(72px *  2); }\n  .grid-item.span-1  { width: calc(100% /  2 - 16px); min-width: calc(72px *  1); }\n}\n\n/* Screens wide enough to accomodate a sidebar */\n@media (min-width: 1280px) {\n  .grid-item.span-12 { width: calc(100% * 12 / 12 - 16px); min-width: calc(72px * 12); }\n  .grid-item.span-11 { width: calc(100% * 11 / 12 - 16px); min-width: calc(72px * 11); }\n  .grid-item.span-10 { width: calc(100% * 10 / 12 - 16px); min-width: calc(72px * 10); }\n   /* XXX: off by one pixel bcause it sometimes did not fit exactly into its space */\n  .grid-item.span-9  { width: calc(100% *  9 / 12 - 17px); min-width: calc(72px *  9); }\n  .grid-item.span-8  { width: calc(100% *  8 / 12 - 16px); min-width: calc(72px *  8); }\n  .grid-item.span-7  { width: calc(100% *  7 / 12 - 16px); min-width: calc(72px *  7); }\n  .grid-item.span-6  { width: calc(100% *  6 / 12 - 16px); min-width: calc(72px *  6); }\n  .grid-item.span-5  { width: calc(100% *  5 / 12 - 16px); min-width: calc(72px *  5); }\n  .grid-item,\n  .grid-item.span-4  { width: calc(100% *  4 / 12 - 16px); min-width: calc(72px *  4); }\n  .grid-item.span-3  { width: calc(100% *  3 / 12 - 16px); min-width: calc(72px *  3); }\n  .grid-item.span-2  { width: calc(100% *  2 / 12 - 16px); min-width: calc(72px *  2); }\n  .grid-item.span-1  { width: calc(100% *  1 / 12 - 16px); min-width: calc(72px *  1); }\n}"
