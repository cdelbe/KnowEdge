            "text": "<$scrollable fallthrough=\"no\" class=\"tc-sidebar-scrollable mdc-persistent-drawer\">\n\n<nav class=\"tc-sidebar-header sidebar-wrapper\">\n\n<div class=\"sidebar-content mdc-toolbar-fixed-adjust\">\n\n<div class=\"mdc-list-group\">\n\n<nav class=\"mdc-list\">\n\n<$list filter=\"[all[shadows+tiddlers]tag[$:/tags/DrawerTemplate]!has[draft.of]]\">\n\n<$transclude mode=\"inline\"/>\n\n</$list>\n\n</nav>\n\n<nav class=\"mdc-list\">\n\n</nav>\n\n</div>\n\n</div>\n\n<div class=\"sidebar-footer\">\n\n</div>\n\n</nav>\n\n</$scrollable>"
